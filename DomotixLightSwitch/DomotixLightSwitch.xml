<?xml version="1.0"?>
<device>
	<developer>Pochet Romuald</developer>
	<product>DomotixLightController</product>
	<pwrdownmode>false</pwrdownmode>
	<config>
		<reg name="sensor" id="11">
			<param name="delay" type="num">
				<position>2.0</position>
				<size>2.0</size>
			</param>
		</reg>
	</config>
	<regular>
		<reg name="sensor" id="12">
			<endpoint name="temperature" type="num" dir="inp">
				<size>4.0</size>
				<units>
					<unit name="°C" factor="1" offset="0"/>
				</units>
			</endpoint>
		</reg>
	</regular>
</device>
