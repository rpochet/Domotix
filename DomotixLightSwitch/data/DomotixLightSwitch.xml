<?xml version="1.0"?>
<device>
	<developer>Pochet Romuald</developer>
	<product>DomotixLightSwitch</product>
	<!-- pwrdownmode: power-down flag. If true, this device sleeps between transmissions.
    Otherwise, it continuously listens to the SWAP network -->
	<pwrdownmode>false</pwrdownmode>
	<config>
		<reg name="config" id="11">
			<param name="sensorDelay" type="num">
				<position>2.0</position>
				<size>2.0</size>
			</param>
		</reg>
	</config>
	<regular>
		<reg name="output0" id="13">
			<endpoint name="level" type="num" dir="inp">
				<size>1.0</size>
				<units>
					<unit name="%" factor="0.0039" offset="0"/>
				</units>
			</endpoint>
		</reg>
		<reg name="output1" id="14">
			<endpoint name="level" type="num" dir="inp">
				<size>1.0</size>
				<units>
					<unit name="%" factor="0.0039" offset="0"/>
				</units>
			</endpoint>
		</reg>
	</regular>
</device>
