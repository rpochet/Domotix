{
  "appenders": [
    {
      "type": "logLevelFilter",
      "level": "INFO",
      "appender": 
      {
        "type": "dateFile",
        "filename": "logs/domotix.log",
        "pattern": "-yyyy-MM-dd",
        "alwaysIncludePattern": true
      }
    }
  ],
  "levels": {
    "[all]": "INFO"
  }
}
